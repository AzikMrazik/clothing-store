{
  "name": "clothing-store-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@mui/x-date-pickers": "^7.28.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-slick": "^0.23.13",
    "date-fns": "^2.30.0",
    "dompurify": "^3.2.5",
    "framer-motion": "^12.6.3",
    "jsdom": "^24.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.0",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "styled-components": "^6.0.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "security:audit": "npm audit --production",
    "security:fix": "npm audit fix --production",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "check-types": "tsc --noEmit",
    "check-updates": "npx npm-check-updates",
    "update-deps": "npx npm-check-updates -u && npm install",
    "build:prod": "NODE_ENV=production tsc && vite build",
    "prebuild:prod": "npm run security:audit && npm run check-types",
    "clean": "rimraf dist coverage",
    "scan:security": "npx snyk test",
    "scan:licences": "npx license-checker --production",
    "csp-check": "csp-html-webpack-plugin --analyze ./dist/index.html"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.25.9",
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.6",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitejs/plugin-react": "^4.0.0",
    "csp-html-webpack-plugin": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "rimraf": "^5.0.5",
    "snyk": "^1.1235.0",
    "vite": "^6.2.5"
  }
}
